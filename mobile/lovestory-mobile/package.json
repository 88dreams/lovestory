{
  "name": "lovestory-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:update-snapshots": "jest -u",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:update": "jest --updateSnapshot",
    "test:clear": "jest --clearCache",
    "test:debug": "jest --verbose --detectOpenHandles",
    "test:unit": "jest src/test/unit --watch",
    "test:integration": "jest src/test/integration --watch",
    "test:e2e": "jest src/test/e2e --runInBand",
    "test:services": "jest src/services --watch",
    "test:components": "jest src/components --watch",
    "test:auth": "jest src/test/integration/auth.test.tsx --watch",
    "test:auth:coverage": "jest src/test/integration/auth.test.tsx --coverage",
    "test:auth:debug": "jest src/test/integration/auth.test.tsx --watch --verbose",
    "test:staged": "jest --bail --findRelatedTests",
    "test:changed": "jest --onlyChanged",
    "test:coverage:open": "jest --coverage && open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.4.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@reduxjs/toolkit": "^2.5.1",
    "axios": "^1.7.9",
    "expo": "~52.0.28",
    "expo-apple-authentication": "^7.1.3",
    "expo-auth-session": "^6.0.3",
    "expo-random": "^14.0.1",
    "expo-status-bar": "~2.0.1",
    "expo-web-browser": "^14.0.2",
    "react": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "^2.22.1",
    "react-native-safe-area-context": "^5.1.0",
    "react-native-screens": "^4.6.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.8",
    "@types/react-native": "^0.73.0",
    "@types/react-navigation": "^3.4.0",
    "@types/react-test-renderer": "^19.0.0",
    "@types/redux": "^3.6.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.3",
    "jsdom": "^26.0.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "msw": "^2.7.0",
    "node-fetch": "2",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3"
  },
  "private": true
}
